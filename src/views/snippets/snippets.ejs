  <h1>VIEW ALL SNIPPETS</h1>
        <div class ="snippetpage">
            <% if (snippet?.length > 0) { %>
                <% snippet.forEach(obj => { %>
                    <div class="snippetwrapper">
                    <p class="titletext"><%= obj.title %></p>
                        <pre><code id="codetext" class="<%=obj.language%>"><%=obj.body%></code></pre>
                        <p id="languagetext">Language: <%=obj.language%> // Contributor: <%=obj.author%></p><br>
                     
                    </div>
                <% }) %>
              <% } else { %>
                  <p>No products to display.</p>
              <% } %>
        </div>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.4.0/highlight.min.js"></script>
        <script>hljs.initHighlightingOnLoad()</script>